
{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/2",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "functions": {
    "node": {
      "includeFiles": "node_modules/puppeteer/.local-chromium/**/*"
    }
  },
  "build": {
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
  }
}
