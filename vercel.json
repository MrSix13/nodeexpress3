{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/2",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "functions": {
		"index.js": {
			"maxDuration": 60
		}
	},
  "build": {
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
  }
}
